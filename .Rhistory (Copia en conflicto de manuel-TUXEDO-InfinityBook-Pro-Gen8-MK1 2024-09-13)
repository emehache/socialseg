gpkg_to_spatial <- function(file, loc20 = T){
input<-st_read(file, quiet = T)
if (loc20) input <- input %>%
.[grep("*20$",.$LOCALIDAD),]
input<-st_transform(input, crs=32721) %>%
as_Spatial()
}
